class Solution {
public:
    string largestOddNumber(string num) {
        int cnt = -1;
        string ans = "";
        for(int i = num.size() - 1; i >=0 ; i--){
            if(num[i] % 2 == 1){
                cnt = i;
                break;
            }
        }
        for(int i = 0; i <= cnt; i++){
            ans.push_back(num[i]);
        }
        return ans;
    }
};